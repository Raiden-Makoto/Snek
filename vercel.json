{
  "buildCommand": "bash setup-and-build-web.sh",
  "outputDirectory": "build-web",
  "framework": null,
  "installCommand": "echo 'No npm install needed'",
  "headers": [
    {
      "source": "/(.*\\.wasm)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/wasm"
        }
      ]
    },
    {
      "source": "/(.*\\.data)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/octet-stream"
        }
      ]
    }
  ]
}

